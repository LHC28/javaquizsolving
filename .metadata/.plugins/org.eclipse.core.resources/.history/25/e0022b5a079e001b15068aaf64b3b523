package variable_quiz;

public class ½ÇÇè2 {

	public static void main(String[] args) {

		for(int i=0; i<5; i++) {
			for(int j=0; j<5; j++) {
				if(j==2 && i!=3) {
					System.out.print(i+" ");
				}else if(i==2) {
					System.out.print(j+" ");
				}else {
					System.out.print("* ");
				}
			}
			System.out.println();
		}

	}

}

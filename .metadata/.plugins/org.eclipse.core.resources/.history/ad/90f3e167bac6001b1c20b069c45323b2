package 과제;

public class Animal {

	public String name;
	public int age;
	public int height;
	
	public Animal(String name, int age, int heigth) {
		this.name = name;
		this.age = age;
		this.height = height;
	}
	
	public void eat() {
		System.out.println("밥을 먹기 시작한다.");
	}
	
	public void sleep() {
		System.out.println("잠을 자기 시작한다.");
	}
}
